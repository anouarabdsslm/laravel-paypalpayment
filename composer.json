    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Anouar Abdessalam",
            "email": "dtekind@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "~4.0",
        "ext-curl": "*",
        "ext-json": "*",
        "paypal/rest-api-sdk-php": "0.15.1"
    },
    "require-dev":{
        "phpspec/phpspec": "~2.0"
    },
    "autoload": {
        "psr-0": {
            "Anouar\\Paypalpayment": "src/"
        }
    },
    "minimum-stability": "dev"
}
